generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Lista {
  id        Int      @id @default(autoincrement())
  descricao String
  data      DateTime
  status    String
  ordem     Int?
}
